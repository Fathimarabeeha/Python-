   "source": [
    "#COUNT THE OCCURRENCES OF 'A'\n",
    "a=(input(\"Enter the list of names:\").split())\n",
    "count=0\n",
    "for name in a:\n",
    "    count+=name.lower().count('a')\n",
    "print(\"Occurrences of a:\",count)"
   ]
